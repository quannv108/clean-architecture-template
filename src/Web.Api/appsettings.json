{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "main-read-write": "",
    "main-read-only": "",
    "hangfire": ""
  },
  "Redis": {
    "ConnectionString": "",
    "InstanceName": "CleanArchitecture:"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Jwt": {
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 60,
    "RefreshTokenExpirationInDays": 7,
    "RsaPrivateKey": "",
    "RsaPublicKey": ""
  },
  "Encryption": {
    "Key": "",
    "CurrentVersion": 1,
    "LegacyKeys": {}
  },
  "App": {
    "BaseUrl": "http://localhost:5000"
  },
  "Outbox": {
    "PollingIntervalMs": 300000,
    "BatchSize": 20
  },
  "Email": {
    "Provider": "Dummy",
    "FromAddress": "no-reply@example.com",
    "Ses": {
      "Region": "us-east-1",
      "AccessKeyId": "",
      "SecretAccessKey": ""
    }
  },
  "Sms": {
    "Provider": "Dummy",
    "Twilio": {
      "AccountSid": "",
      "AuthToken": "",
      "MessagingServiceSid": "MGXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX",
      "FromNumber": "+15551234567"
    }
  },
  "Push": {
    "Provider": "Local",
    "Fcm": {
      "ServiceAccountKeyPath": "",
      "ProjectId": ""
    }
  },
  "FileStorage": {
    "Provider": "Local",
    "MaxFileSizeBytes": 10485760,
    "AllowedContentTypes": [
      "application/pdf",
      "image/jpeg",
      "image/png",
      "image/gif",
      "application/msword",
      "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
      "application/vnd.ms-excel",
      "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
    ],
    "Local": {
      "BasePath": "./uploads"
    },
    "S3": {
      "BucketName": "",
      "Region": "us-east-1",
      "AccessKeyId": "",
      "SecretAccessKey": ""
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:3000"
    ]
  }
}